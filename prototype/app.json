    "android": {
      "package": "com.unibo.carkeeper",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e2f62017-c7cc-4948-a3d0-486506834ab3"
      }
    },
    "owner": "ilcorsdev"
  }
